{
    "name": "BasicIOStream",
    "version": "1.0.1",
    "description": "This Library is an easy to use Library for lcd print functions, and serial print",
    "keywords": ["IOStream", "Serial", "LCD", "I2CLCD"],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/braydenanderson2014/C-Arduino-Libraries/tree/main/iostream.git"
        }
    ],
    "authors": [
        {
            "name": "Brayden Anderson",
            "email": "braydenanderson2014@gmail.com",
            "url": "https://github.com/braydenanderson2014"
        }
    ],
    "dependencies": {
        "arduino-libraries/SD": "1.2.4",
        "Arduino": "*",
        "LiquidCrystal": "*",
        "Wire": "*",
        "SPI": "*",
        "LiquidCrystal_I2C": "*"
    },
    "include": [
        "Wire.h"
    ],
    "examples": [
        {
            "name": "Example",
            "base": "Examples",
            "files": [
                "Example.cpp"
            ]
        }
    ],
    "headers": [
